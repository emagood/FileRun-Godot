; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="save-and-load"
run/main_scene="res://ecenas/Main.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.5")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://asets/fondo-godot.png"
config/icon="res://powered-cog-64.png"
boot_splash/minimum_display_time=3000

[display]

window/size/viewport_width=800
window/size/viewport_height=800
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=6

[file_customization]

folder_colors={
"res://asets/": "green",
"res://captura/": "yellow",
"res://ecenas/": "red",
"res://saves/": "gray"
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_ticks_per_second=35

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
